<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:App/etc/routes.xsd">  <!--Use router 'standard' for frontend route-->
    <router id="admin">
        <!--Define a custom route with id and frontName-->
        <route id="example" frontName="example">
            <!--The module which this route match to-->
            <module name="Tigren_HelloWorld"/>
        </route>
    </router>
</config> 
